{
  "compilerOptions": {
    "noImplicitAny": true,
    "outDir": "./bundle",
    "rootDir": "./src",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "jsx": "react",
    "baseUrl": ".",
    "types": ["react/next", "react-dom/next"],
    "target": "es2016",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,          
    "skipLibCheck": true,
  }
}
